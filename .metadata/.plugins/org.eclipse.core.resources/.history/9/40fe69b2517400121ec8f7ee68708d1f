package com.empsi.inepabor;

import java.util.ArrayList;
import java.util.List;

public class ParsedDataSet {
	private String title = null;
	private String scroll = null;
	private String view = null;
	private List<ParsedDataSet> entries = new ArrayList<ParsedDataSet>();

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getScroll() {
		return scroll;
	}

	public void setScroll(String scroll) {
		this.scroll = scroll;
	}

	public String getView() {
		return view;
	}

	public void setView(String view) {
		this.view = view;
	}

	public List<ParsedDataSet> getEntries() {
		return entries;
	}

	public void setEntries(List<ParsedDataSet> entries) {
		this.entries = entries;
	}
	
	public void addEntry(ParsedDataSet dataSet){
		this.entries.add(dataSet);
	}
}
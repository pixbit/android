package com.empsi.inepabor;

import java.util.ArrayList;
import java.util.List;

import android.util.Log;

public class ParsedExampleDataSet {
	private String extractedString = null;
	private String tagString = null;
	private int extractedInt = 0;
	private List<String> extractedStrings = new ArrayList<String>();
	private List<String> tagStrings = new ArrayList<String>();

	public String getExtractedString() {
		return extractedString;
	}

	public void setExtractedTag(String tagString) {
		this.setTagString(tagString);
		Log.d("ParsedExampleDataSet.setExtractedTag()", tagString);
		this.tagStrings.add(tagString);
	}
	
	public void setExtractedString(String extractedString) {
		this.extractedString = extractedString;
//		Log.d("ParsedExampleDataSet.setExtractedString()", extractedString);
		this.extractedStrings.add(extractedString);
	}

	public int getExtractedInt() {
		return extractedInt;
	}
	public void setExtractedInt(int extractedInt) {
		this.extractedInt = extractedInt;
//		Log.d("ParsedExampleDataSet.setExtractedInt()", Integer.toString(this.extractedInt));
	}
	
	public String toString(){
		return "ExtractedString = " + this.extractedString
				+ "nExtractedInt = " + this.extractedInt;
	}
	
	public List<String> getList(){
		return this.extractedStrings;
	}

	public String getTagString() {
		return tagString;
	}

	public void setTagString(String tagString) {
		this.tagString = tagString;
	}
}